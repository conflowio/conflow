iterator1 iterator {
    count = 10
    i1 it
}

iterator2 iterator {
    count = 10
    i2 it
}

sleep1 sleep {
    i1 := i1.value
    duration = 50ms
}

sleep2 sleep {
    i2 := i2.value
    duration = 100ms
}

@triggers ["sleep2"]
println {
    value = string(sleep1.i1) + " " + string(sleep2.i2)
}
